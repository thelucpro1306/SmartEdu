@model IEnumerable<SmartEDU_2.Models.THONGBAO>
@{
    Layout = null;
}
@foreach (var item in Model)
{
    <div class="new-item col-xs-12 no-padding-lr first">
        <div class="new-item-brbt">
            <div class="col-xs-12 col-sm-5 no-padding-l home-blog-content-left">
                <div class="new-img">
                    <a href="#" title="#"><img src="//bizweb.dktcdn.net/thumb/medium/100/238/035/articles/rsz-vtc14.png?v=1504081610130"></a>
                </div>
            </div>
            <div class="col-xs-12 col-sm-7 no-padding-r home-blog-content-right">
                <div class="home-blog-content-right-div">
                    <h3 class="new-name"><a href="@Url.Action("Detail","Notification",new {id=item.MATHONGBAO })" title="@item.TIEUDE">@item.TIEUDE</a></h3>
                    <p class="new-info"><span><i class="fa fa-calendar"></i> @item.NGAYTHONGBAO</span> <span><i class="fa fa-comment-o"></i> 0 Bình luận</span></p>
                </div>
            </div>
        </div>
    </div>
}

