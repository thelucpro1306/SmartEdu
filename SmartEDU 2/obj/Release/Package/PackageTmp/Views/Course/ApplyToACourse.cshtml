                            </li>

                            <li>Đăng ký khóa học</li>

                        </ol>
                    </div>
                </div>
            </div>
        </div>
        @ViewBag.Notifi
        <div class="container-contact100">
            <div class="wrap-contact100" align="center">
                @using (Html.BeginForm("Index", "GoToSchool"))
                {
                    <form class="contact100-form validate-form">
                        <span class="contact100-form-title">
                            ĐIỀN THÔNG TIN - XÁC NHẬN ĐĂNG KÝ
                        </span>
                        <div class="col-lg-12">
                            <div class="col-lg-6 col-sm-12">
                                <div style="border:solid;color:#0d72ba;padding:20px;border-radius:20px">


                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Họ tên của bé</span>
                                        <input class="input100" type="text" name="hotenbe" placeholder="VD: Bé Thành Luân" required value="@tt.HOTEN">
                                        <span class="focus-input100"></span>
                                    </div>

                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Giới tính:</span> <br />
                                        <select style="width:100%;height:40px;font-size:18px;color:#333333;border-radius: 20px;padding: 0 10px;">
                                            <option value="gtNam">Nam</option>
                                            <option value="gtNu">Nữ</option>
                                        </select>
                                        <span class="focus-input100"></span>
                                    </div>

                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Năm sinh</span>
                                        <input class="input100" type="date" name="namsinh" required placeholder="19/12/2015">
                                        <span class="focus-input100"></span>
                                    </div>

                                    <div class="dangnhap wrap-input100 validate-input">
                                        <span class="label-input100">Họ tên người giám hộ</span>
                                        <input class="input100" type="text" name="hotencha" placeholder="VD: Nguyễn Văn B">
                                        <span class="focus-input100"></span>
                                    </div>
                                </div>

                            </div>
                            <div class="col-lg-6 col-sm-12">
                                <div style="border:solid;color:#0d72ba;padding:20px;border-radius:20px">

                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Mã khóa học :</span>
                                        <input class="input100" type="text" name="makhoahoc" required value="@item.MAKHOAHOC" readonly>
                                        <span class="focus-input100"></span>
                                    </div>

                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Tên khóa học :</span>
                                        <input class="input100" type="text" name="tenkhoahoc" required value="@item.TENKHOAHOC" readonly>
                                        <span class="focus-input100"></span>
                                    </div>

                                    <div class="wrap-input100 validate-input">
                                        <span class="label-input100">Học phí (VNĐ) :</span>
                                        <input class="input100" type="text" name="hocphi" required value="@item.HOCPHI" readonly>
                                        <span class="focus-input100"></span>
                                    </div>
                                    <div class="dangnhap wrap-input100 validate-input">
                                        <span class="label-input100">Số điện thoại liên lạc</span>
                                        <input class="input100" type="text" name="sdt" placeholder="VD: 09012345678">
                                        <span class="focus-input100"></span>
                                    </div>

                                </div>
                            </div>
                        </div>


                        <div class="container-contact100-form-btn col-lg-12" style="padding: 10px 30px;">
                            <div class="wrap-contact100-form-btn">
                                <div class="contact100-form-bgbtn"></div>
                                <button class="contact100-form-btn">
                                    Đăng Ký Nhập Học
                                    <i class="fa fa-long-arrow-right m-l-7" aria-hidden="true"></i>
                                </button>
                            </div>
                        </div>




                    </form>
                }
            </div>
        </div>

        <div id="dropDownSelect1"></div>
        <!--===============================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/jquery/jquery-3.2.1.min.js"></script>
        <!--=========~/RegisterToGoToSchool/======================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/animsition/js/animsition.min.js"></script>
        <!--=========~/RegisterToGoToSchool/======================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/bootstrap/js/popper.js"></script>
        <script src="~/RegisterToGoToSchool/vendor/bootstrap/js/bootstrap.min.js"></script>
        <!--=========~/RegisterToGoToSchool/======================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/select2/select2.min.js"></script>
        <script>
            $(".selection-2").select2({
                minimumResultsForSearch: 20,
                dropdownParent: $('#dropDownSelect1')
            });
        </script>

        <!--===============================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/daterangepicker/moment.min.js"></script>
        <script src="~/RegisterToGoToSchool/vendor/daterangepicker/daterangepicker.js"></script>
        <!--=========~/RegisterToGoToSchool/======================================================================================-->
        <script src="~/RegisterToGoToSchool/vendor/countdowntime/countdowntime.js"></script>
        <!--=========~/RegisterToGoToSchool/======================================================================================-->
        <script src="~/RegisterToGoToSchool/js/main.js"></script>

        <!-- Global site tag (gtag.js) - Google Analytics -->
        <script async src="https://www.googletagmanager.com/gtag/js?id=UA-23581568-13"></script>
        <script>
            window.dataLayer = window.dataLayer || [];
            function gtag() { dataLayer.push(arguments); }
            gtag('js', new Date());
            gtag('config', 'UA-23581568-13');
        </script>

    </body>
</html>
}



