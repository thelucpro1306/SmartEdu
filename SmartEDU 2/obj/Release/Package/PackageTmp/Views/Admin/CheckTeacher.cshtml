
@{
    ViewBag.Title = "Điểm danh";
    Layout = "~/Views/Shared/_LayoutAd.cshtml";
}


<div class="panel box-shadow-none content-header">
    <div class="panel-body">
        <div class="col-md-12">
            <h3 class="animated fadeInLeft">Danh sách giáo viên</h3>
            <p class="animated fadeInDown">
                Admin <span class="fa-angle-right fa"></span> Danh sách giáo viên
            </p>
        </div>
    </div>
</div>
<div class="col-md-12 top-20 padding-0">
    <div class="col-md-12">
        <div class="panel">
            <div class="panel-heading"><h3>Danh sách giáo viên</h3></div>
            @*<p>
                <button class="btn btn-success" type="button">@Html.ActionLink("Thêm tài khoản", "Register")</button>
            </p>*@
            <div class="panel-body">
                <div class="responsive-table">
                    <table id="datatables-example" class="table table-striped table-bordered" width="100%" cellspacing="0">
                        <thead>
                            <tr>
                                <th>Số thứ tự</th>
                                <th>Tên giáo viên</th>
                                <th>Thời gian</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td class="column1">@item.MAGV</td>
                                    <td class="column2">@item.HOTEN</td>
                                    <td class="column3">@item.NGAYSINH</td>
              
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>